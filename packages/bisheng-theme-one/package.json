{
  "name": "bisheng-theme-one",
  "version": "0.2.35",
  "description": "The one theme for bisheng.",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "babel ./src --out-dir ./lib && shx cp -R src/static/* lib/static",
    "postpublish": "shx rm -rf lib"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "Firefox ESR",
              "> 1%",
              "ie >= 8",
              "iOS >= 8",
              "Android >= 4"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjycui/bisheng-theme-one.git"
  },
  "keywords": [
    "bisheng",
    "theme",
    "one"
  ],
  "author": "Benjy Cui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjycui/bisheng-theme-one/issues"
  },
  "homepage": "https://github.com/benjycui/bisheng-theme-one#readme",
  "engines": {
    "node": ">=8.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "bisheng": "^0.28.4",
    "react": "^16.0.0",
    "shx": "^0.3.2"
  },
  "dependencies": {
    "bisheng-plugin-description": "^0.1.4",
    "react-document-title": "^2.0.1"
  },
  "peerDependencies": {
    "bisheng": "^0.28.3",
    "react": "^16.0.0"
  }
}
