{
  "name": "bisheng",
  "version": "0.26.0",
  "description": "Transform Markdown(and other static files with transformers) into a SPA website using React.",
  "bin": {
    "bisheng": "./bin/bisheng"
  },
  "files": [
    "lib",
    "bin",
    "router.js",
    "collect.js"
  ],
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run build && shx cp src/template.html lib/template.html",
    "postpublish": "npm run clean"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjycui/bisheng.git"
  },
  "keywords": [
    "markdown",
    "spa",
    "website",
    "blog",
    "react"
  ],
  "author": "Benjy Cui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjycui/bisheng/issues"
  },
  "homepage": "https://github.com/benjycui/bisheng#readme",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.6.0",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "commander": "^2.9.0",
    "css-loader": "~0.28.0",
    "exist.js": "^0.3.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gh-pages": "^0.12.0",
    "history": "^3.0.0",
    "jsonml-to-react-element": "^1.0.0",
    "jsonml.js": "^0.1.0",
    "less": "~2.7.2",
    "less-loader": "~4.0.5",
    "loader-utils": "^1.1.0",
    "mark-twain": "^2.0.0",
    "mkdirp": "^0.5.1",
    "node-prismjs": "^0.1.0",
    "nprogress": "^0.2.0",
    "nunjucks": "^2.5.2",
    "postcss": "^6.0.11",
    "postcss-loader": "~2.0.6",
    "prismjs": "^1.4.1",
    "ramda": "^0.22.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^3.0.0",
    "resolve": "^1.1.7",
    "rucksack-css": "~1.0.2",
    "ts-loader": "^2.0.3",
    "url-loader": "~0.5.8",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chalk": "^1.1.3",
    "rimraf": "^2.5.4",
    "shx": "^0.2.2"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
